{
  "name": "aetasaal-api",
  "private": true,
  "version": "1.0.0",
  "description": "Aetasaal API",
  "scripts": {
    "build-ts": "tsc",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "nodemon -r dotenv/config bin/server dotenv_config_path=src/config/.env --node-env=development | bunyan -o simple",
    "start:dev": "nodemon -r dotenv/config bin/server dotenv_config_path=src/config/.env --node-env=development | bunyan -o simple",
    "start:qa": "nodemon -r dotenv/config bin/server dotenv_config_path=src/config/.env --node-env=qa | bunyan -o simple",
    "watch": "nodemon bin/server | bunyan -o simple",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "pretest": "npm run lint",
    "sqlz:createdb": "sequelize db:create",
    "sqlz:migrate": "sequelize db:migrate",
    "sqlz:undo": "sequelize db:migrate:undo",
    "sqlz:new": "sequelize migration:create",
    "sqlz:seednew": "sequelize seed:create",
    "sqlz:seed": "sequelize db:seed:all",
    "sqlz:seedp": "sequelize db:seed --seed"
  },
  "author": "Muhammad Owais <mok.developer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "6.3.1",
    "boom": "7.2.0",
    "bunyan": "1.8.12",
    "cloudinary": "^1.13.2",
    "convict": "4.3.1",
    "dotenv": "^6.2.0",
    "fcm-push": "^1.1.3",
    "fs-extra": "^7.0.1",
    "joi": "13.4.0",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "2.2.0",
    "koa": "2.5.2",
    "koa-body": "4.0.4",
    "koa-bunyan-logger": "2.0.0",
    "koa-compose": "4.1.0",
    "koa-helmet": "4.0.0",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "koa-static": "4.0.3",
    "koa2-cors": "^2.0.6",
    "lodash": "4.17.10",
    "mustache": "^3.0.1",
    "nodemailer": "^5.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "2.3.2",
    "randexp": "git://github.com/fent/randexp.js.git#v0.5.2",
    "sequelize": "4.37.10",
    "sequelize-cli": "4.0.0",
    "ts-node": "6.0.5",
    "typescript": "2.9.2",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/boom": "7.2.0",
    "@types/bunyan": "1.8.2",
    "@types/convict": "4.2.0",
    "@types/dotenv": "4.0.3",
    "@types/fs-extra": "^5.0.4",
    "@types/joi": "13.3.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/jwt-decode": "2.2.1",
    "@types/koa": "2.0.46",
    "@types/koa-helmet": "3.1.2",
    "@types/koa-router": "7.0.23",
    "@types/koa2-cors": "^2.0.1",
    "@types/koa-compose": "3.2.0",
    "@types/mustache": "^0.8.32",
    "@types/nodemailer": "4.6.2",
    "@types/sequelize": "4.27.20",
    "@types/uuid": "3.4.3",
    "@types/node": "10.12.0",
    "jest": "21.1.0",
    "nodemon": "1.12.0",
    "prettier": "^1.15.3",
    "ts-jest": "21.0.1",
    "tslint": "5.10.0"
  },
  "engineStrict": true,
  "engines": {
    "node": "10.2.1"
  }
}
